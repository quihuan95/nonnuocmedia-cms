{
    "kind": "collectionType",
    "collectionName": "categories",
    "info": {
        "singularName": "category",
        "pluralName": "categories",
        "displayName": "Category"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "title": {
            "type": "string",
            "required": true
        },
        "slug": {
            "type": "uid",
            "targetField": "title"
        },
        "description": {
            "type": "text"
        },
        "is_featured": {
            "type": "boolean",
            "default": true
        },
        "posts": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::post.post",
            "inversedBy": "categories"
        },
        "old_db_id": {
            "type": "biginteger"
        },
        "thumb_nail": {
            "type": "media",
            "conditions": {
                "visible": {
                    "==": [
                        {
                            "var": "is_featured"
                        },
                        true
                    ]
                }
            },
            "multiple": false,
            "allowedTypes": [
                "images",
                "files"
            ]
        },
        "content": {
            "type": "blocks",
            "conditions": {
                "visible": {
                    "==": [
                        {
                            "var": "is_featured"
                        },
                        true
                    ]
                }
            }
        }
    }
}
