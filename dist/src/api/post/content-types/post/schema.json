{
    "kind": "collectionType",
    "collectionName": "posts",
    "info": {
        "singularName": "post",
        "pluralName": "posts",
        "displayName": "Post"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "Title": {
            "type": "string",
            "required": true
        },
        "Description": {
            "type": "text"
        },
        "slug": {
            "type": "uid",
            "targetField": "Title",
            "required": true
        },
        "Thumbnail": {
            "type": "media",
            "multiple": false,
            "required": false,
            "allowedTypes": [
                "images",
                "files"
            ]
        },
        "Content": {
            "type": "richtext"
        },
        "Date": {
            "type": "datetime"
        },
        "tags": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::tag.tag",
            "inversedBy": "posts"
        },
        "Publish": {
            "type": "boolean",
            "required": false,
            "default": true
        },
        "IsFeatured": {
            "type": "boolean"
        },
        "Views": {
            "type": "biginteger",
            "default": "0"
        },
        "categories": {
            "type": "relation",
            "relation": "manyToMany",
            "target": "api::category.category",
            "mappedBy": "posts"
        },
        "old_db_id": {
            "type": "biginteger"
        }
    }
}
